          <maml:name>toastOnSuccess</maml:name>
          <maml:Description>
            <maml:para>Creates a toast when it succeeds to set the clipboard text.</maml:para>
          </maml:Description>
          <dev:type>
            <maml:name>SwitchParameter</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>False</dev:defaultValue>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>
    <command:parameters>
