          <maml:name>ServerSideProcessing</maml:name>
          <maml:Description>
            <maml:para>Turns on server side processing for the grid. This disables client-side filtering, sort and paging.</maml:para>
          </maml:Description>
          <dev:type>
            <maml:name>SwitchParameter</maml:name>
            <maml:uri />
