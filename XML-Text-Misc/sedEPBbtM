    <command:details>
      <command:name>Invoke-UDEvent</command:name>
      <command:verb>Invoke</command:verb>
      <command:noun>UDEvent</command:noun>
      <maml:description>
        <maml:para>Performs a clientside "click" event on the specified id, if the element is clickable.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>Performs a clientside "click" event on the specified id, if the element is clickable.</maml:para>
