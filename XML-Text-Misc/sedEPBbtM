	$Cookie = Get-UDCookie -Name "MyCookie"

	New-UDInputAction -Toast $Cookie.Value
}</dev:code>
        <dev:remarks>
          <maml:para>Returns the value of MyCookie as a Toast when the user clicks the submit button.</maml:para>
