        <maml:name>RenderWhenActive</maml:name>
        <maml:Description>
          <maml:para>Only render tabs when active.</maml:para>
        </maml:Description>
        <command:parameterValue required="false" variableLength="false">SwitchParameter</command:parameterValue>
